<emailInterceptionTask port="3737">
   <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailOrRule">
      <rules>
         <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailAndRule">
            <rules>
               <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailSubjectRule" matchType="PLAIN" subject="ads"/>
               <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailContentRule" matchType="WILDCARD" content="recruitment of * membership"/>
            </rules>
         </receivedEmailRule>
         <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailAndRule">
            <rules>
               <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailSubjectRule" matchType="WILDCARD" subject="可口可乐*广告"/>
               <receivedEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.receivedEmail.ReceivedEmailContentRule" matchType="PLAIN" content="健康好味道"/>
            </rules>
         </receivedEmailRule>
      </rules>
   </receivedEmailRule>
   <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailAndRule">
      <rules>
         <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailOrRule">
            <rules>
               <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailToRule" matchType="REGEX" to="[a-zA-Z]*[0-1]*@qq.com"/>
               <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailContentRule" matchType="WILDCARD" content="会员*优惠"/>
            </rules>
         </sentEmailRule>
         <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailOrRule">
            <rules>
               <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailToDomainRule" matchType="WILDCARD" toDomain="*.hotmail.com"/>
               <sentEmailRule class="net.pickapack.net.mitm.emailInterception.model.rule.sentEmail.SentEmailContentRule" matchType="PLAIN" content="微软公司"/>
            </rules>
         </sentEmailRule>
      </rules>
   </sentEmailRule>
</emailInterceptionTask>